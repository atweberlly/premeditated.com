{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off",
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": "error"
  }
}
